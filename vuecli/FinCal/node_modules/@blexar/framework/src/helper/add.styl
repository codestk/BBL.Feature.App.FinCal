.u
  &-sticky
    position: sticky
    top: $margin

  &-float-right
    float: right

  &-float-left
    float: left

  &-flex
    display: flex

  &-justify-center
    justify-content: center

  &-justify-end
    justify-content: flex-end

  &-justify-start
    justify-content: flex-start

  &-justify-around
    justify-content: space-around

  &-justify-between
    justify-content: space-between

  &-align-center
    align-items: center

  &-align-end
    align-items: flex-end

  &-align-start
    align-items: flex-start

  &-align-baseline
    align-items: baseline

  &-align-stretch
    align-items: stretch
