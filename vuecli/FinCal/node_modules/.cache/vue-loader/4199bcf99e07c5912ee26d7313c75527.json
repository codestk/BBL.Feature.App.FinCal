lor: #003fbd;\n    letter-spacing: 0.8px;\n    text-decoration: none;\n    background-color: transparent;\n    cursor: pointer;\n    transition: all 0.3s;\n    padding: 10px;\n}\n\n.default-tabs__item_active {\n    color: #003fbd;\n}\n\n.default-tabs__item:hover {\n    color: #003fbd;\n}\n\n.default-tabs__item:focus {\n    outline: none;\n    color: #003fbd;\n}\n\n.default-tabs__item:first-child {\n    margin-left: 0;\n}\n\n.default-tabs__item:last-child {\n    margin-right: 0;\n}\n\n.default-tabs__active-line {\n    position: absolute;\n    bottom: 0;\n    left: 0;\n    height: 2px;\n    background-color: #003fbd;\n    transition: all 0.3s;\n}\n\n.tabs-content {\n    margin-top: 30px;\n    font-size: 20px;\n}\n\n.result {\n    margin-top: 20px;\n    border-radius: 10px;\n    background: #233a58;\n}\n\n    .result p {\n        line-height: 2rem;\n    }\n",{"version":3,"sources":["HomeLoanPlan.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AA2HA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA","file":"HomeLoanPlan.vue","sourceRoot":"src/components/HomeLoanPlan","sourcesContent":["<template>\r\n    <div class=\"HomeLoan\">\r\n        <vue-form :state=\"formstate\" @submit.prevent=\"onSubmit\" class=\"general-form\">\r\n            <div class=\"row\">\r\n                <div class=\"col-centered col-md-12\">\r\n                    <p class=\"section-header\">แผนคำนวณยอดผ่อนชำระสินเชื่อบ้าน</p>\r\n                    <validate auto-label class=\"form-group required-field\">\r\n                        <div class=\"fincal-group\" :class=\"fieldClassName(formstate.LimitAmount)\">\r\n                            <div class=\"col-md-8 col-sm-8 col-xs-8 fincal-text-left\">\r\n                                <label>วงเงินที่ขอสินเชื่อ</label>\r\n                            </div>\r\n\r\n                            <div class=\"col-md-4 col-sm-4 col-xs-4 fincal-text-right\">\r\n                                <!-- <input type=\"text\" name=\"name\" class=\"form-control\" required v-model.lazy=\"model.CurrentAge\"> -->\r\n                                <input name=\"LimitAmount\"\r\n                                       v-model.lazy=\"model.LimitAmount\"\r\n                                       type=\"number\"\r\n                                       required\r\n                                       min=\"0\"\r\n                                       max=\"99999999999\"\r\n                                       onKeyPress=\"if(this.value.length==12) return false;\">\r\n                            </div>\r\n                        </div>\r\n\r\n                        <field-messages name=\"LimitAmount\" show=\"$touched || $submitted\" class=\"form-control-feedback\" :class=\"fieldClassName(formstate.LimitAmount)\">\r\n                            <div slot=\"required\">Name is a required field</div>\r\n                            <div slot=\"min\">กรุณากรอกจำนวนเงิน 0-10,000,000 บาท</div>\r\n                            <div slot=\"max\">กรุณากรอกจำนวนเงิน 0-10,000,000 บาท</div>\r\n                        </field-messages>\r\n                    </validate>\r\n\r\n                    <validate auto-label class=\"form-group required-field\">\r\n                        <div class=\"fincal-group\" :class=\"fieldClassName(formstate.Term)\">\r\n                            <div class=\"col-md-8 col-sm-8 col-xs-8 fincal-text-left\">\r\n                                <label>ระยะเวลา</label>\r\n                            </div>\r\n\r\n                            <div class=\"col-md-4 col-sm-4 col-xs-4 fincal-text-right\">\r\n                                <input name=\"Term\"\r\n                                       v-model.lazy=\"model.Term\"\r\n                                       type=\"number\"\r\n                                       required\r\n                                       min=\"0\"\r\n                                       max=\"30\"\r\n                                       onKeyPress=\"if(this.value.length==2) return false;\">\r\n                            </div>\r\n                        </div>\r\n\r\n                        <field-messages name=\"Term\" show=\"$touched || $submitted\" class=\"form-control-feedback\" :class=\"fieldClassName(formstate.Term)\">\r\n                            <div slot=\"required\">Name is a required field</div>\r\n                            <div slot=\"min\">กรุณากรอกระยะเวลา 1-30 ปี</div>\r\n                            <div slot=\"max\">กรุณากรอกระยะเวลา 1-30 ปี</div>\r\n                        </field-messages>\r\n                    </validate>\r\n                </div>\r\n                <!-- <button class='btn btn-primary btn-sm btn-block text-center '   v-on:click='AcceptCanCelOrder '  > Shoot</button> -->\r\n                <div class=\"col-centered col-xs-11 col-sm-8 col-md-10 col-lg-5\">\r\n                    <button class=\"btn btn-primary btn-sm btn-block text-cente\" type=\"submit\">คำนวณ</button>\r\n                </div>\r\n            </div>\r\n\r\n            <div class=\"row\" id=\"result\" v-show=\"false\">\r\n                <div class=\"col-md-12 col-centered\">\r\n                    <div class=\"result\">\r\n                        <p>ยอดผ่อนชำระรายเดือน {{ this.twoDigit(this.result.LowIncome)}} บาท</p>\r\n                        <p>รายได้ต่อเดือนขั้นต่ำ {{this.twoDigit(this.result.PaymentAmount)}} บาท</p>\r\n                    </div>\r\n                </div>\r\n            </div>\r\n        </vue-form>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\n    import callapi from \"../Common/apicall.js\";\r\n    export default {\r\n        data: function () {\r\n            return {\r\n                formstate: {},\r\n                model: {\r\n                    LimitAmount: 5000000,\r\n                    Term: 20\r\n                },\r\n                result: {\r\n                    LowIncome: 0,\r\n                    PaymentAmount: 0\r\n                }\r\n            };\r\n        },\r\n        methods: {\r\n            twoDigit: function (val) {\r\n                return val.toFixed(2).toString().replace(/\\B(?=(\\d{3})+(?!\\d))/g, \",\")\r\n            },\r\n            fieldClassName: function (field) {\r\n                if (!field) {\r\n                    return \"\";\r\n                }\r\n                if ((field.$touched || field.$submitted) && field.$valid) {\r\n                    return \"has-success\";\r\n                }\r\n                if ((field.$touched || field.$submitted) && field.$invalid) {\r\n                    return \"has-danger\";\r\n                }\r\n            },\r\n            onSubmit: function () {\r\n                $('#result').fadeOut(200);\r\n                if (this.formstate.$invalid) {\r\n                    return;\r\n                }\r\n                var response = callapi.post(Server + EndPoint, HomeLoan, this.model, ocpkey).then(response => {\r\n                    this.result.LowIncome = response.data.LowIncome;\r\n                    this.result.PaymentAmount = response.data.PaymentAmount;\r\n                    $('#result').fadeIn(400);\r\n                }).catch(e => {\r\n                    this.errors.push(e);\r\n                })\r\n            }\r\n        }\r\n    };\r\n</script>\r\n\r\n<style>\r\n\r\n    .pad-section {\r\n        margin-top: 10px;\r\n        margin-bottom: 10px;\r\n    }\r\n\r\n    .section-header {\r\n        margin-top: 10px;\r\n        margin-bottom: 20px;\r\n    }\r\n\r\n    .default-tabs {\r\n        position: relative;\r\n        margin: 0 auto;\r\n    }\r\n\r\n    .default-tabs__item {\r\n        display: inline-block;\r\n        margin: 12px 0px;\r\n        font-size: 1rem;\r\n        color: #003fbd;\r\n        letter-spacing: 0.8px;\r\n        text-decoration: none;\r\n        background-color: transparent;\r\n        cursor: pointer;\r\n        transition: all 0.3s;\r\n        padding: 10px;\r\n    }\r\n\r\n    .default-tabs__item_active {\r\n        color: #003fbd;\r\n    }\r\n\r\n    .default-tabs__item:hover {\r\n        color: #003fbd;\r\n    }\r\n\r\n    .default-tabs__item:focus {\r\n        outline: none;\r\n        color: #003fbd;\r\n    }\r\n\r\n    .default-tabs__item:first-child {\r\n        margin-left: 0;\r\n    }\r\n\r\n    .default-tabs__item:last-child {\r\n        margin-right: 0;\r\n    }\r\n\r\n    .default-tabs__active-line {\r\n        position: absolute;\r\n        bottom: 0;\r\n        left: 0;\r\n        height: 2px;\r\n        background-color: #003fbd;\r\n        transition: all 0.3s;\r\n    }\r\n\r\n    .tabs-content {\r\n        margin-top: 30px;\r\n        font-size: 20px;\r\n    }\r\n\r\n    .result {\r\n        margin-top: 20px;\r\n        border-radius: 10px;\r\n        background: #233a58;\r\n    }\r\n\r\n        .result p {\r\n            line-height: 2rem;\r\n        }\r\n</style>\r\n"]}]}